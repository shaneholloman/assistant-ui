{
  "name": "@assistant-ui/react-ai-sdk",
  "version": "1.3.7",
  "description": "Vercel AI SDK adapter for assistant-ui",
  "keywords": [
    "ai-sdk",
    "vercel",
    "assistant-ui",
    "react",
    "ai",
    "chat",
    "openai",
    "streaming"
  ],
  "author": "AgentbaseAI Inc.",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "aui-source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "aui-build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@ai-sdk/react": "^3.0.75",
    "ai": "^6.0.73",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "@assistant-ui/react": "^0.12.10",
    "@types/react": "*",
    "assistant-cloud": "*",
    "react": "^18 || ^19"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "assistant-cloud": {
      "optional": true
    }
  },
  "devDependencies": {
    "@assistant-ui/react": "workspace:*",
    "@assistant-ui/x-buildutils": "workspace:*",
    "@testing-library/react": "^16.3.2",
    "@types/json-schema": "^7.0.15",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "assistant-stream": "workspace:*",
    "jsdom": "^28.0.0",
    "react": "^19.2.4",
    "vitest": "^4.0.18"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "homepage": "https://www.assistant-ui.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/assistant-ui/assistant-ui.git",
    "directory": "packages/react-ai-sdk"
  },
  "bugs": {
    "url": "https://github.com/assistant-ui/assistant-ui/issues"
  }
}
