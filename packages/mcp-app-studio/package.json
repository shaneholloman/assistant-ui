{
  "name": "mcp-app-studio",
  "version": "0.7.1",
  "description": "Build interactive apps for MCP Apps hosts (ChatGPT, Claude Desktop, etc.)",
  "keywords": [
    "chatgpt",
    "openai",
    "claude",
    "anthropic",
    "mcp",
    "widget",
    "app",
    "create",
    "scaffold",
    "studio"
  ],
  "author": "AgentbaseAI Inc.",
  "license": "MIT",
  "type": "module",
  "bin": {
    "mcp-app-studio": "./bin/mcp-app-studio.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "aui-source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./cli": {
      "aui-source": "./src/cli/index.ts",
      "types": "./dist/cli/index.d.ts",
      "default": "./dist/cli/index.js"
    },
    "./core": {
      "aui-source": "./src/core/index.ts",
      "types": "./dist/core/index.d.ts",
      "default": "./dist/core/index.js"
    },
    "./mcp": {
      "aui-source": "./src/platforms/mcp/index.ts",
      "types": "./dist/platforms/mcp/index.d.ts",
      "default": "./dist/platforms/mcp/index.js"
    }
  },
  "files": [
    "bin",
    "dist",
    "src",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "aui-build",
    "preview:sync": "tsx scripts/sync-template.ts",
    "preview:install": "cd .preview && npm install",
    "preview:dev": "cd .preview && npm run dev",
    "preview": "tsx scripts/sync-template.ts && cd .preview && npm run dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:smoke": "vitest run --config vitest.smoke.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:scaffold": "bash scripts/test-scaffold.sh",
    "typecheck": "tsc --noEmit",
    "prepack": "pnpm run build"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "picocolors": "^1.1.1",
    "tar": "^7.5.7"
  },
  "peerDependencies": {
    "@modelcontextprotocol/ext-apps": ">=0.4.0",
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "@modelcontextprotocol/ext-apps": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@modelcontextprotocol/ext-apps": "^1.0.1",
    "@assistant-ui/x-buildutils": "workspace:*",
    "@types/node": "^25.2.1",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/tar": "^6.1.13",
    "@vitest/coverage-v8": "^4.0.18",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://www.assistant-ui.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/assistant-ui/assistant-ui.git",
    "directory": "packages/mcp-app-studio"
  },
  "bugs": {
    "url": "https://github.com/assistant-ui/assistant-ui/issues"
  },
  "engines": {
    "node": ">=20.9.0"
  }
}
